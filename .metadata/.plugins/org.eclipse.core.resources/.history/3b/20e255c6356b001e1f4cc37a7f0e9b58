package VariablesandMethods;

public class StaticVar {
	
	String name;
	String address;
	String city;

	StaticVar(String name, String address, String city)
	{
		this.name=name;
	}
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		StaticVar obj = new StaticVar("Bob","CanarabankLayout","Bengaluru");
		StaticVar obj1 = new StaticVar("Jack","Malleswaram","Bengaluru");
		
	}

}

package JavaCollections;

public class ArrayList {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Integer :  The Integer class wraps a value of the primitive type int in an object. 
		//An object of type Integercontains a single field whose type is int.
		//Arrays have fixed size and array list can be added dynamically
	//	You can access and insert any value in any index
		
		
		java.util.ArrayList<String> a = new java.util.ArrayList<String>();
		a.add("rahul");
		a.add("Java");
		System.out.println(a);
		a.add(0, "Students");
		System.out.println(a);
		System.out.println(a.get(2));
		System.out.println(a.indexOf("rahul"));
		System.out.println(a.isEmpty());
		System.out.println(a.size());
		//a.remove(1);
		//a.remove("Java");
		System.out.println(a);
	}

}
